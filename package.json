{
  "name": "node-dev-env",
  "version": "1.0.0",
  "description": "Simple node development environment for api",
  "main": "app.js",
  "scripts": {
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "DEBUG=app:* nodemon --exec babel-node ./src/app.js",
    "open:src2": "DEBUG=app:* babel-node ./src/app.js",
    "start-message": "DEBUG=app:* babel-node ./buildScripts/startMessege",
    "dev": "npm-run-all --parallel start-message open:src lint:watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "build-message": "DEBUG=app:* babel-node ./buildScripts/buildMessage",
    "prebuild": "npm-run-all clean-dist test lint build-message",
    "build": "babel src --out-dir dist",
    "start": "NODE_ENV=production node ./dist/app.js",
    "localtunnel": "lt --port 3000 --subdomain euvi3168461",
    "share": "npm-run-all --parallel start-message open:src lint:watch localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jumiranda5/node-dev-env"
  },
  "author": "Juliana Miranda",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.2",
    "google-auth-library": "^7.10.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0",
    "redisgraph.js": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "debug": "^4.3.2",
    "node": "16.6.0",
    "npm": "7.20.3"
  },
  "keywords": [
    "node",
    "starter",
    "boilerplate"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-watch": "^7.0.0",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5"
  }
}
